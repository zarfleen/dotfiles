[color]
	ui = true
	diff = true 
	status = true
	branch = true
[user]
	name = Brad Burns
	email = bburns@burnix.org
[credential]
	helper = cache
[alias]
	co = checkout
	count = !git shortlog -sn
[core]
	editor = vim

[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
